{
  "watch": ["src"],
  "ext": "ts",
  "ignore": ["src/**/*.spec.ts", "src/**/*.test.ts"],
  "exec": "ts-node -r tsconfig-paths/register src/main.ts",
  "env": {
    "NODE_ENV": "development"
  },
  "delay": 1000,
  "signal": "SIGTERM",
  "killTimeout": 3000,
  "restartable": "rs",
  "verbose": false
}

